{
  "name": "Better Internet",
  "version": "2.0",
  "description": "",
  "permissions": [
    "nativeMessaging",
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "alarms",
    "proxy",
    "contextMenus"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "16": "internet_16.png",
      "32": "internet_32.png",
      "48": "internet_48.png",
      "128": "internet_128.png"
    }
  },
  "icons": {
    "16": "internet_16.png",
    "32": "internet_32.png",
    "48": "internet_48.png",
    "128": "internet_128.png"
  },
  "manifest_version": 2
}